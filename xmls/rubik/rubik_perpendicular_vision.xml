                    <geom name="sticker:neg_y" class="sticker" material="sticker_G" size="0.008 0.0001 0.008" pos="0.0 -0.01 0.0" />
                </body>
                <joint name="cubelet:driver:neg_y" type="hinge" pos="0 0 0" axis="0 1 0"/>
